{
  "name": "moaw-database",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run -s create:db",
    "build": "tsc",
    "watch": "tsc -w",
    "create:db": "node dist/src/create-workshop-db.js",
    "format": "prettier --write '(src|scripts)/**/*.{ts,json,scss}'",
    "prepare": "npm run -s build"
  },
  "private": true,
  "dependencies": {
    "cxa-track": "^2.1.0",
    "fast-glob": "^3.2.12",
    "marked": "^4.0.19",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "@types/node": "^16.11.62",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  }
}
